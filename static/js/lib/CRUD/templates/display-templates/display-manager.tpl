<div data-wireframe="{{entityType}}-view" class="overlay WIREFRAME">
  <div class="header">
    <button class="display do-hide is-small">
      <span class="T">hide</span>
    </button>
  </div>
  <div class="body" style="bottom: 49px;">
  </div>
  <div class="footer actions">
    <div class="left">
      <div class="when-overlay-expanded">
        <button class="do-collapse">
          <span class="text T">Collapse »</span>
        </button>
      </div>
      <div class="when-overlay-not_expanded">
        <button class="do-expand">
          <span class="text T">« Expand</span>
        </button>
      </div>
    </div>
    <div class="right">
      <button class="do-select">
        <span class="text T">Select</span>
      </button>
      <button class="do-select edit default">
        <span class="text T">Edit</span>
      </button>
    </div>
  </div>
</div>
